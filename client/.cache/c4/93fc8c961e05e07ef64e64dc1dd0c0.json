            return this.$emit('taskEdit', obj)\r\n            },\r\n            editTask() {\r\n                return this.$emit('editTask', this.dataTask)\r\n            }\r\n\r\n        }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n.bodycard{\r\n    \r\n    height: 2%;\r\n}\r\n.card {\r\n  margin-top: 10px;\r\n  margin-left: 10px;\r\n  margin-right: 15px;\r\n  border-left: 15px solid #99c5e2;\r\n  border-top-right-radius: 13px;\r\n}\r\n\r\n#first {\r\n  background: rgb(255,255,255);\r\n  background: linear-gradient(135deg, rgba(255,255,255,.4) 0%,    rgba(200,200,200,.5) 100%);\r\n  backdrop-filter: blur(10pxpx);\r\n}\r\n\r\n.desc{\r\n    font-weight: 900;\r\n    font-size: large;\r\n}\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"8e2237ff148bfcad7a1a7d6372522473","cacheData":{"env":{}}}